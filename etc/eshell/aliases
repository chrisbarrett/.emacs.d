alias d docker $*
alias dco docker-compose
alias e fasd-find-file -1 ${eshell-flatten-and-stringify $*}
alias f find-file $1
alias ga git add $*
alias gb git branch -v $*
alias gc (call-interactively 'magit-commit)
alias gco (call-interactively 'magit-checkout)
alias gd (call-interactively 'magit-diff-popup)
alias gf (call-interactively 'magit-fetch-popup)
alias gl (call-interactively 'magit-log-current)
alias gm (call-interactively 'magit-merge-popup)
alias gmv git mv $*
alias gp (call-interactively 'magit-push-popup)
alias gr (call-interactively 'magit-rebase-popup)
alias gref (call-interactively 'magit-reflog-current)
alias grh git reset --hard $*
alias grm git rm $*
alias gs git show $*
alias gup git remote update -p && git pull --rebase
alias k tree -L 2 -C $*
alias l ls $*
alias la ls -lA $*
alias ll ls -l $*
alias lsa ls -lah $*
alias pf projectile-find-file
alias rmr rm -r $*
alias s magit-status
alias t proced
alias tree tree -I target -I node_modules $*
alias v (progn (prodigy) nil)
