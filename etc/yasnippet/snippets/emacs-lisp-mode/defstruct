# key: ds
# name: defstruct
# condition: (yas-funcs-bolp)
# --
(cl-defstruct (${1:Name}
                 (:constructor ${2:$1} (${3:arglist})))
    "${4:Description}${3:$(unless (equal "slots" yas/text) (yas/process-docstring yas/text))}"
    ${3:$(s-join " " (-map 'symbol-name (yas/simplify-arglist yas/text)))})
